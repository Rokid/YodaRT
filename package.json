{
  "name": "yodart",
  "version": "7.27.0",
  "scripts": {
    "lint": "npm run lint-js && npm run lint-cc",
    "lint-js": "standard",
    "lint-cc": "tools/clang-format",
    "website": "jsdoc -c jsdoc.config -R README.md -r . -d website/docs -u website/tutorials",
    "pretest": "tools/runtime-install -a",
    "test": "tools/test"
  },
  "config": {
    "unsafe-perm": true
  },
  "engine": {
    "shadow": "0.9.x"
  },
  "manifest": {
    "apilevel": 0.3
  },
  "devDependencies": {
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "standard": "^11.0.1",
    "tap-json": "^1.0.0",
    "tap-nyan": "^1.1.0",
    "tap-spec": "^5.0.0"
  },
  "standard": {
    "ignore": [
      "website",
      "node_modules"
    ]
  }
}
